{
  "[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "breadcrumbs.enabled": false,
  "debug.toolBarLocation": "docked",
  "editor.codeActionsOnSave": { "source.fixAll.eslint": true },
  "editor.cursorStyle": "line-thin",
  "editor.fontFamily": "'SF Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontWeight": "400",
  "editor.formatOnSave": true,
  "editor.lineHeight": 17,
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.overviewRulerBorder": false,
  "editor.renderIndentGuides": false,
  "editor.renderLineHighlight": "all",
  "editor.scrollBeyondLastLine": false,
  "editor.semanticHighlighting.enabled": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "[Default Light+]": {
      "comments": "#878687",
      "functions": "#3F6F7C",
      "keywords": "#0146AB",
      "numbers": "#0E7CB9",
      "strings": "#dc2501",
      "types": "#7319A4",
      "variables": "#010000",
      "textMateRules": [
        {
          "scope": [
            "punctuation.definition.template-expression.begin",
            "punctuation.definition.template-expression.end",
            "string.regexp.js",
            "string.regexp.js punctuation.definition.string.begin.js",
            "string.regexp.js punctuation.definition.string.end.js",
            "punctuation.definition.raw.markdown",
            "markup.inline.raw.string.markdown"
          ],
          "settings": {
            "foreground": "#FF5433"
          }
        },
        {
          "scope": [
            "support.variable",
            "variable.other.constant",
            "entity.name.type.module",
            "punctuation.separator.key-value"
          ],
          "settings": {
            "foreground": "#010000"
          }
        },
        {
          "scope": [
            "storage.type.function.arrow",
            "keyword.operator.assignment",
            "meta.brace.round",
            "meta.brace.square",
            "punctuation.definition.block",
            "punctuation.definition.parameters",
            "punctuation.definition.string.begin.markdown",
            "punctuation.definition.string.end.markdown",
            "punctuation.definition.metadata.markdown",
            "keyword.operator.expression",
            "keyword.operator.logical",
            "keyword.operator.comparison",
            "storage.modifier",
            "punctuation.definition.typeparameters.begin",
            "punctuation.definition.typeparameters.end",
            "keyword.operator.type",
            "punctuation.section.embedded.begin",
            "punctuation.section.embedded.end"
          ],
          "settings": {
            "foreground": "#0070F7"
          }
        },
        {
          "scope": [
            "punctuation.definition.heading.markdown",
            "entity.name.section.markdown",
            "punctuation.definition.list.begin.markdown"
          ],
          "settings": {
            "foreground": "#0146AB",
            "fontStyle": ""
          }
        },
        {
          "scope": [
            "punctuation.definition.bold.markdown",
            "markup.bold.markdown"
          ],
          "settings": {
            "foreground": "#5D8E03"
          }
        },
        {
          "scope": "string.other.link.title.markdown",
          "settings": {
            "foreground": "#0E7CB9"
          }
        },
        {
          "scope": "markup.underline.link.markdown",
          "settings": {
            "foreground": "#DC2501"
          }
        },
        {
          "scope": ["entity.other.attribute-name", "constant.language.null"],
          "settings": {
            "foreground": "#0E7CB9"
          }
        },
        {
          "scope": [
            "punctuation.definition.tag.begin",
            "punctuation.definition.tag.end"
          ],
          "settings": { "foreground": "#7319A4" }
        }
      ]
    },
    "[Default Dark+]": {
      "comments": "#667280",
      "functions": "#75C6B3",
      "keywords": "#67B3FF",
      "numbers": "#66B3FF",
      "strings": "#F26557",
      "types": "#A187DE",
      "variables": "#D9E6F1",
      "textMateRules": [
        {
          "scope": [
            "punctuation.definition.template-expression.begin",
            "punctuation.definition.template-expression.end",
            "string.regexp.js",
            "string.regexp.js punctuation.definition.string.begin.js",
            "string.regexp.js punctuation.definition.string.end.js",
            "punctuation.definition.raw.markdown",
            "markup.inline.raw.string.markdown"
          ],
          "settings": {
            "foreground": "#FFC147"
          }
        },
        {
          "scope": [
            "support.variable",
            "variable.other.constant",
            "entity.name.type.module",
            "punctuation.separator.key-value"
          ],
          "settings": {
            "foreground": "#D9E6F1"
          }
        },
        {
          "scope": [
            "storage.type.function.arrow",
            "keyword.operator.assignment",
            "meta.brace.round",
            "meta.brace.square",
            "punctuation.definition.block",
            "punctuation.definition.parameters",
            "punctuation.definition.string.begin.markdown",
            "punctuation.definition.string.end.markdown",
            "punctuation.definition.metadata.markdown",
            "keyword.operator.expression",
            "keyword.operator.logical",
            "keyword.operator.comparison",
            "storage.modifier",
            "punctuation.definition.typeparameters.begin",
            "punctuation.definition.typeparameters.end",
            "keyword.operator.type",
            "punctuation.section.embedded.begin",
            "punctuation.section.embedded.end"
          ],
          "settings": {
            "foreground": "#9BADC1"
          }
        },
        {
          "scope": [
            "punctuation.definition.heading.markdown",
            "entity.name.section.markdown",
            "punctuation.definition.list.begin.markdown"
          ],
          "settings": {
            "foreground": "#CC9A66",
            "fontStyle": ""
          }
        },
        {
          "scope": [
            "punctuation.definition.bold.markdown",
            "markup.bold.markdown"
          ],
          "settings": {
            "foreground": "#4DA2C2"
          }
        },
        {
          "scope": "string.other.link.title.markdown",
          "settings": {
            "foreground": "#67B3FF"
          }
        },
        {
          "scope": "markup.underline.link.markdown",
          "settings": {
            "foreground": "#F26557"
          }
        },
        {
          "scope": ["entity.other.attribute-name", "constant.language.null"],
          "settings": {
            "foreground": "#66B3FF"
          }
        },
        {
          "scope": [
            "punctuation.definition.tag.begin",
            "punctuation.definition.tag.end"
          ],
          "settings": { "foreground": "#A187DE" }
        }
      ]
    }
  },
  "editor.wordWrap": "on",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "files.associations": { "*.plist": "xml" },
  "git.autofetch": true,
  "git.confirmSync": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "ruby.format": "rubocop",
  "ruby.intellisense": false,
  "ruby.useLanguageServer": true,
  "solargraph.commandPath": "/Users/clay/.rbenv/shims/solargraph",
  "solargraph.checkGemVersion": false,
  "terminal.explorerKind": "external",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.lineHeight": 1.23,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.nativeTabs": true,
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "[Default Light+]": {
      "activityBar.background": "#E4E5E6",
      "activityBar.border": "#C2C3C3",
      "activityBar.foreground": "#0170F5",
      "activityBar.inactiveForeground": "#616161",
      "sideBar.foreground": "#313233",
      "gitDecoration.ignoredResourceForeground": "#666666",
      "sideBar.background": "#E4E5E6",
      "sideBar.border": "#C2C3C3",
      "statusBar.background": "#ffffff",
      "statusBar.noFolderBackground": "#ffffff",
      "statusBar.border": "#C2C3C3",
      "statusBar.foreground": "#313233",
      "tab.border": "#C2C3C3",
      "tab.activeBorder": "#ffffff",
      "editorGroupHeader.tabsBorder": "#C2C3C3",
      "editorLineNumber.foreground": "#A7A7A7",
      "editorLineNumber.activeForeground": "#000000",
      "editor.lineHighlightBorder": "#ffffff00",
      "editor.lineHighlightBackground": "#FFFFE6"
    },
    "[Default Dark+]": {
      "activityBar.background": "#2C2F31",
      "activityBar.border": "#464746",
      "activityBar.foreground": "#0170F5",
      "activityBar.inactiveForeground": "#cccccc",
      "sideBar.foreground": "#ffffff",
      "gitDecoration.ignoredResourceForeground": "#A7A8AA",
      "sideBar.background": "#2C2F31",
      "sideBar.border": "#464746",
      "statusBar.background": "#1E1E1E",
      "statusBar.noFolderBackground": "#1E1E1E",
      "statusBar.border": "#464746",
      "statusBar.foreground": "#ffffff",
      "tab.border": "#464746",
      "tab.activeBorder": "#1E1E1E",
      "editorGroupHeader.tabsBorder": "#464746",
      "editorLineNumber.foreground": "#737478",
      "editorLineNumber.activeForeground": "#ffffff",
      "editor.lineHighlightBorder": "#1E1E1E00",
      "editor.lineHighlightBackground": "#242627"
    }
  },
  "workbench.editor.showIcons": false,
  "workbench.editor.tabCloseButton": "left",
  "window.titleBarStyle": "native",
  "workbench.iconTheme": "macos-modern-big-sur-icon-theme",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.autoDetectColorScheme": true,
  "workbench.colorTheme": "Default Light+"
}
